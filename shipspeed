    Friend WithEvents Label1 As System.Windows.Forms.Label
    Friend WithEvents Label2 As System.Windows.Forms.Label
    Friend WithEvents Label4 As System.Windows.Forms.Label
    Friend WithEvents Label5 As System.Windows.Forms.Label
    Friend WithEvents Label6 As System.Windows.Forms.Label
    Friend WithEvents Label7 As System.Windows.Forms.Label
    Friend WithEvents Label8 As System.Windows.Forms.Label
    Friend WithEvents Label9 As System.Windows.Forms.Label

    Dim ShipSpd, TrlTime_To, TrlTime_To_NL, TrlTime_Back, WindInflu, Price_discount, RiskRate_To, RiskRate_Back, Distance_Inspd As Single
    Dim ShipBp, RelShipSpd_AL, RelShipSpd_AL_To, RelShipSpd_AL_Back, RevShipSpd_AL, MaxShipSpd_AL, RevBookProf_AL_To, RevBookProf_AL_Back, RevShipProf_AL_To, Bookcost_H_To, Bookcost_H_Back, Bookcost_H, RevShipProf_AL_Back, MaxShipProf_AL_To, MaxShipProf_AL_Back, RevShipProf_ALH, MaxShipProf_ALH, RevShipProf_ALH_F, MaxShipProf_ALH_F, Bookcost_To, Bookcost_Back, SingleBookProf, SingleProf_To, SingleProf_Back As Single
    Dim ShipMem, SingleBookLod_To, SingleBookLod_Back, ShipLod, ShipLod_To, ShipLod_Back, ShipTlod, ShipSp, RelShipSp_AL, RelShipSp_AL_To, RelShipSp_AL_Back, GoodInput_To, DragRate, GoodOutput_To, GoodInput_Back, GoodOutput_Back, BookStar_To, BookStar_Back As Integer
    Dim Weth_Cap, Weth_monthcard As Boolean
    Dim RealDis_Sp As Single

    Private Sub Form1_Activated(sender As Object, e As EventArgs) Handles Me.Activated
        
    End Sub
    Private Sub Form1_Load(sender As Object, e As EventArgs) Handles MyBase.Load
        '变量初始化
        TextBox1.Text = 800
        TextBox2.Text = 600
        TextBox3.Text = 0.2
        TextBox4.Text = 200
        CheckBox1.Checked = True
        CheckBox2.Checked = True
        TextBox18.Text = 3000
        TextBox19.Text = 11000
        TextBox6.Text = 7.5
        TextBox7.Text = 70
        TextBox23.Text = 70
        TextBox20.Text = 2100
        TextBox21.Text = 3200
        TextBox22.Text = 0
        CheckBox6.Checked = True
        CheckBox7.Checked = True
        CheckBox8.Checked = True
        TextBox28.Enabled = False
        TextBox29.Enabled = False
        TextBox28.Text = 0
        TextBox29.Text = 0

        ComboBox2.SelectedIndex = 0
        ComboBox3.SelectedIndex = 0

        GroupBox1.Visible = False
        Button1.Enabled = False

        '初始化赋值
        ShipSp = TextBox1.Text
        ShipLod = TextBox2.Text
        ShipMem = TextBox4.Text
        Weth_Cap = CheckBox1.Checked
        Weth_monthcard = CheckBox2.Checked
        GoodInput_To = TextBox18.Text
        GoodOutput_To = TextBox19.Text
        TrlTime_To = TextBox6.Text
        SingleBookLod_To = TextBox7.Text
        SingleBookLod_Back = TextBox23.Text
        GoodInput_Back = TextBox20.Text
        GoodOutput_Back = TextBox21.Text
        DragRate = TextBox22.Text
        RiskRate_To = TextBox3.Text
    End Sub
    Friend WithEvents TextBox1 As System.Windows.Forms.TextBox
    Friend WithEvents TextBox2 As System.Windows.Forms.TextBox
    Friend WithEvents TextBox8 As System.Windows.Forms.TextBox
    Friend WithEvents TextBox9 As System.Windows.Forms.TextBox
    Friend WithEvents TextBox10 As System.Windows.Forms.TextBox
    Friend WithEvents CheckBox1 As System.Windows.Forms.CheckBox
    Friend WithEvents CheckBox2 As System.Windows.Forms.CheckBox
    Friend WithEvents Button1 As System.Windows.Forms.Button
    Friend WithEvents Button2 As System.Windows.Forms.Button
    Public Function Input_Warning(WrongInput)
        If IsNumeric(WrongInput) Then

            Input_Warning = 1
        Else

            MessageBox.Show("请保证输入的是数字")
            ' WrongInput = 1
            Input_Warning = 0

        End If

    End Function


    Private Sub Button1_Click(sender As Object, e As EventArgs) Handles Button1.Click

        '初始化
        Input_Warning(TextBox1.Text)
        Input_Warning(TextBox2.Text)
        Input_Warning(TextBox4.Text)
        Input_Warning(TextBox18.Text)
        Input_Warning(TextBox19.Text)
        Input_Warning(TextBox6.Text)
        Input_Warning(TextBox7.Text)
        Input_Warning(TextBox23.Text)
        Input_Warning(TextBox20.Text)
        Input_Warning(TextBox21.Text)
        Input_Warning(TextBox22.Text)
        Input_Warning(TextBox3.Text)
        Input_Warning(TextBox17.Text)
        Input_Warning(TextBox5.Text)
        Input_Warning(TextBox24.Text)

        If Input_Warning(TextBox1.Text) And _
        Input_Warning(TextBox2.Text) And _
        Input_Warning(TextBox4.Text) And _
        Input_Warning(TextBox18.Text) And _
        Input_Warning(TextBox19.Text) And _
        Input_Warning(TextBox6.Text) And _
        Input_Warning(TextBox7.Text) And _
        Input_Warning(TextBox23.Text) And _
Input_Warning(TextBox20.Text) And _
Input_Warning(TextBox21.Text) And _
Input_Warning(TextBox22.Text) And _
Input_Warning(TextBox3.Text) And _
Input_Warning(TextBox17.Text) And _
Input_Warning(TextBox5.Text) And _
Input_Warning(TextBox24.Text) Then
        Else
            Exit Sub
        End If


        ShipSp = TextBox1.Text
        ShipLod = TextBox2.Text
        ShipMem = TextBox4.Text
        Weth_Cap = CheckBox1.Checked
        Weth_monthcard = CheckBox2.Checked
        GoodInput_To = TextBox18.Text
        GoodOutput_To = TextBox19.Text
        TrlTime_To_NL = TextBox6.Text
        SingleBookLod_To = TextBox7.Text
        SingleBookLod_Back = TextBox23.Text
        GoodInput_Back = TextBox20.Text
        GoodOutput_Back = TextBox21.Text
        DragRate = TextBox22.Text
        RiskRate_To = TextBox3.Text
        BookStar_To = ComboBox2.Text
        BookStar_Back = ComboBox3.Text
        TextBox17.Text = 0
        TextBox5.Text = 0
        TextBox24.Text = 0
        Price_discount = 0.3

        '载货相对速度计算
        If CheckBox7.Checked = False Then
            ShipLod_To = TextBox28.Text
            RelShipSp_AL_To = ShipSp * 0.97 ^ (ShipLod_To / 100)
        Else
            ShipLod_To = ShipLod
        End If
        If CheckBox8.Checked = False Then
            ShipLod_Back = TextBox29.Text
            RelShipSp_AL_Back = ShipSp * 0.97 ^ (ShipLod_Back / 100)
        Else
            ShipLod_Back = ShipLod
        End If

        RealDis_Sp = ShipSp * TrlTime_To_NL
        If CheckBox7.Checked = True Then
            RelShipSp_AL = ShipSp * 0.97 ^ (ShipLod / 100) '未航海士的影响可加入
            ShipBp = RelShipSp_AL * ShipLod / 10

        Else

            RelShipSp_AL = RelShipSp_AL_To
        End If
        RelShipSpd_AL = RelShipSp_AL * 0.02

        If Weth_Cap And Weth_monthcard Then
            MaxShipSpd_AL = RelShipSpd_AL * 1.08 '未加入航海士的影响
        ElseIf Weth_Cap Then
        ElseIf Weth_monthcard And Weth_Cap = False Then
            MaxShipSpd_AL = RelShipSpd_AL * 1.08
        End If

        '载货相对时间计算
        If TextBox27.Enabled = True And TextBox27.Text <> "" Then
            TrlTime_To = TextBox27.Text
        Else
            TrlTime_To = RealDis_Sp / RelShipSp_AL
            TextBox27.Text = TrlTime_To
        End If

        If DragRate = 0 And CheckBox7.Checked = True And CheckBox8.Checked = True Then
            TrlTime_Back = TrlTime_To / (1 - WindInflu) * (1 + WindInflu) * (1 - DragRate / 100)
        ElseIf DragRate = 0 And CheckBox8.Checked = False Then
            TrlTime_Back = RealDis_Sp / RelShipSp_AL_Back * (1 - DragRate / 100)
        Else
            TrlTime_Back = RealDis_Sp / ShipSp * (1 - DragRate / 100)
        End If


        '载货利润评估
        Dim Loadtimes_H_To, Loadtimes_H_Back, Loadtimes_H, Max_Loadtimes_H_To, Max_Loadtimes_H_Back As Single
        Dim Loading_Time_H As Single

        Loading_Time_H = 0.2    '入港消耗12秒
        WindInflu = 0.05    '风向系数5%

        SingleProf_To = (GoodOutput_To - GoodInput_To) / 10000
        SingleProf_Back = (GoodOutput_Back - GoodInput_Back) / 10000

        RevShipProf_AL_To = ShipLod_To * SingleProf_To
        '拖航最多可带20箱货物
        If Not DragRate Then
            RevShipProf_AL_Back = ShipLod_Back * SingleProf_Back
        Else
            RevShipProf_AL_Back = 20 * SingleProf_Back
        End If

        MaxShipProf_AL_To = ShipLod_To * SingleProf_To * (Price_discount + 1)
        MaxShipProf_AL_Back = ShipLod_Back * SingleProf_Back * (Price_discount + 1)

        '风险自动评估与天数成递增
        If CheckBox5.Checked Then
            RiskRate_To = 1 - 0.985 ^ TrlTime_To
            TextBox3.Text = RiskRate_To
            RiskRate_Back = 1 - 0.985 ^ TrlTime_Back
        Else
            RiskRate_To = TextBox3.Text
            RiskRate_Back = RiskRate_To
        End If

        Loadtimes_H_To = (60 / (TrlTime_To + TrlTime_Back + Loading_Time_H * 2)) + 1
        Loadtimes_H_Back = Loadtimes_H_To - 1

        Max_Loadtimes_H_To = (60 / ((TrlTime_To + TrlTime_Back) * 1.15) + Loading_Time_H * 2) + 1  '未加入顺风因素为15%，未加入航海士因素
        Max_Loadtimes_H_Back = Max_Loadtimes_H_To

        RevShipProf_ALH = RevShipProf_AL_To * Loadtimes_H_To / (RiskRate_To + 1) + RevShipProf_AL_Back * Loadtimes_H_Back / (RiskRate_Back + 1)
        MaxShipProf_ALH = MaxShipProf_AL_To * Max_Loadtimes_H_To / (RiskRate_To / 2 + 1) + MaxShipProf_AL_Back * Max_Loadtimes_H_Back / (RiskRate_To / 2 + 1)



        '星书消耗计算
        Bookcost_To = ShipLod / SingleBookLod_To - 1
        If DragRate > 0 Then
            Bookcost_Back = 0
        Else
            Bookcost_Back = ShipLod / SingleBookLod_Back - 1
        End If

        RevBookProf_AL_To = RevShipProf_AL_To / Bookcost_To
        If Bookcost_Back >= 1 Then
            RevBookProf_AL_Back = RevShipProf_AL_Back / Bookcost_Back
        Else
            RevBookProf_AL_Back = 0
        End If

        Bookcost_H_To = Bookcost_To * Loadtimes_H_To
        Bookcost_H_Back = Bookcost_Back * Loadtimes_H_Back
        Bookcost_H = Bookcost_H_To + Bookcost_H_Back

        '星书分级计算
        Dim RevShipProf_To(3), RevShipProf_Back(3), RevShipProf_ALH_A(3) As Single
        Dim BokCost_To(3), BokCost_Back(3) As Integer

        If BookStar_To = 1 Then
            RevShipProf_To(0) = RevShipProf_AL_To
            BokCost_To(0) = Bookcost_To
        ElseIf BookStar_To = 2 Then
            RevShipProf_To(1) = RevShipProf_AL_To
            BokCost_To(1) = Bookcost_To
        ElseIf BookStar_To = 3 Then
            RevShipProf_To(2) = RevShipProf_AL_To
            BokCost_To(2) = Bookcost_To
        End If
        If DragRate <> 0 Then
            RevShipProf_Back(0) = 0
            RevShipProf_Back(1) = 0
            RevShipProf_Back(2) = 0
        Else
            If BookStar_Back = 1 Then
                RevShipProf_Back(0) = RevShipProf_AL_Back
                BokCost_Back(0) = Bookcost_Back
            ElseIf BookStar_Back = 2 Then
                RevShipProf_Back(1) = RevShipProf_AL_Back
                BokCost_Back(1) = Bookcost_Back
            ElseIf BookStar_Back = 3 Then
                RevShipProf_Back(2) = RevShipProf_AL_Back
                BokCost_Back(2) = Bookcost_Back
            End If
        End If

        If BookStar_To = 1 Then
            TextBox17.Text = Bookcost_H_To
        ElseIf BookStar_To = 2 Then
            TextBox5.Text = Bookcost_H_To
        ElseIf BookStar_To = 3 Then
            TextBox24.Text = Bookcost_H_To
        End If

        If DragRate = 0 Then
            If BookStar_Back = 1 Then
                TextBox17.Text = Int(TextBox17.Text + Bookcost_H_Back) + 1
            ElseIf BookStar_Back = 2 Then
                TextBox5.Text = Int(TextBox5.Text + Bookcost_H_Back) + 1
            ElseIf BookStar_Back = 3 Then
                TextBox24.Text = Int(TextBox24.Text + Bookcost_H_Back) + 1
            End If
        Else
            Bookcost_H_Back = 0
        End If

        '星书分级利润计算
        Dim SingleBookProf_Ttl(3) As Single

        RevShipProf_ALH_A(0) = RevShipProf_To(0) * Loadtimes_H_To / RiskRate_To + RevShipProf_Back(0) * Loadtimes_H_Back / RiskRate_Back
        SingleBookProf_Ttl(0) = RevShipProf_ALH_A(0) / 60 / ((BokCost_To(0) + 1) * Int(Loadtimes_H / 2 + 1) + (BokCost_Back(0) + 1) * Int(Loadtimes_H / 2))

        RevShipProf_ALH_A(1) = RevShipProf_To(1) * Loadtimes_H_To / RiskRate_To + RevShipProf_Back(1) * Loadtimes_H_Back / RiskRate_Back
        SingleBookProf_Ttl(1) = RevShipProf_ALH_A(1) / 60 / ((BokCost_To(1) + 1) * Int(Loadtimes_H / 2 + 1) + (BokCost_Back(1) + 1) * Int(Loadtimes_H / 2))

        RevShipProf_ALH_A(2) = RevShipProf_To(2) * Loadtimes_H_To / RiskRate_To + RevShipProf_Back(2) * Loadtimes_H_Back / RiskRate_Back
        SingleBookProf_Ttl(2) = RevShipProf_ALH_A(2) / 60 / ((BokCost_To(2) + 1) * Int(Loadtimes_H / 2 + 1) + (BokCost_Back(2) + 1) * Int(Loadtimes_H / 2))

        '结果输出
        TextBox8.Text = ShipBp
        TextBox9.Text = RelShipSp_AL
        TextBox10.Text = RelShipSpd_AL
        TextBox11.Text = MaxShipSpd_AL
        TextBox12.Text = RevShipProf_AL_To
        TextBox13.Text = MaxShipProf_AL_To
        TextBox14.Text = RevShipProf_ALH
        TextBox15.Text = MaxShipProf_ALH
        TextBox16.Text = SingleBookProf_Ttl(0)
        TextBox25.Text = SingleBookProf_Ttl(1)
        TextBox26.Text = SingleBookProf_Ttl(2)



        '内部变量还原
        For i = 0 To 3
            RevShipProf_To(i) = 0
            RevShipProf_Back(i) = 0
            RevShipProf_ALH_A(i) = 0
            BokCost_To(i) = 0
            BokCost_Back(i) = 0
        Next

    End Sub


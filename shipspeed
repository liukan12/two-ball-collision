    Private Sub CheckBox7_CheckedChanged(sender As Object, e As EventArgs) Handles CheckBox7.CheckedChanged
        If CheckBox7.Checked = True Then
            TextBox28.Enabled = False
        Else
            TextBox28.Enabled = True
            If IsNumeric(TextBox28.Text) Then
                ShipLod_To = TextBox28.Text
            Else
                TextBox28.Text = 0
                ShipLod_To = TextBox28.Text
            End If
        End If
    End Sub
    Private Sub CheckBox8_CheckedChanged(sender As Object, e As EventArgs) Handles CheckBox8.CheckedChanged
        If CheckBox8.Checked = True Then
            TextBox29.Enabled = False
        Else
            TextBox29.Enabled = True
            If IsNumeric(TextBox29.Text) Then
                ShipLod_Back = TextBox29.Text
            Else
                TextBox29.Text = 0
                ShipLod_Back = TextBox29.Text
            End If

        End If
    End Sub

    Private Sub CheckBox6_CheckedChanged(sender As Object, e As EventArgs) Handles CheckBox6.CheckedChanged
        If CheckBox6.Checked = True Then
            TextBox27.Enabled = False
        Else
            TextBox27.Enabled = True
            If IsNumeric(TextBox27.Text) Then
                TrlTime_To = TextBox27.Text
            Else
                TextBox27.Text = TrlTime_To_NL

            End If
        End If
    End Sub


    Private Sub CheckBox4_CheckedChanged(sender As Object, e As EventArgs) Handles CheckBox4.CheckedChanged
        If CheckBox4.Checked = True Then
            GroupBox1.Visible = True
        Else
            GroupBox1.Visible = False
        End If
    End Sub
